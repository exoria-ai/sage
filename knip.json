{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "lib/**/*.ts",
    "scripts/mcp-dev-server.ts"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "lib/**/*.ts"
  ],
  "ignore": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "scripts/parse-*.ts",
    "scripts/scrape-*.ts",
    "scripts/test-*.ts",
    "docs/**"
  ],
  "ignoreDependencies": [
    "eslint",
    "eslint-config-next",
    "tailwindcss",
    "cheerio",
    "@types/cheerio",
    "turndown",
    "@types/turndown"
  ],
  "rules": {
    "duplicates": "off"
  }
}
